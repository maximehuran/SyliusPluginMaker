parameters:
    level: max
    paths:
        - %rootDir%/src/

    checkMissingIterableValueType: false
    checkGenericClassInNonGenericObjectType: false

    excludePaths:
        # Makes PHPStan crash
        - 'src/DependencyInjection/{{ pluginPascalCaseName|replace({'Plugin': 'Extension'}) }}.php'

        # Test dependencies
        - 'tests/Application/**/*'
